<div class="container clearfix">
  <div class="col-12">
    <div class="single-blog">
      <h1><%= blog.title %></h1>
      <span class="entry-meta">
        <i class="far fa-clock"></i>
        <%= blog.createdOn %>
      </span>
      <div class="col-12 padding-0 content">
        <p>
          <%= blog.description %>
        </p>
      </div>
    </div>
  </div>
</div>
